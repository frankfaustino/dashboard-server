  },
  "homepage": "https://github.com/frankfaustino/dashboard-server#readme",
  "dependencies": {
    "apollo-server": "^2.9.15",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.1.4",
    "merge-graphql-schemas": "^1.7.6",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
